{
  "name": "@immowelt/eslint-config-immowelt-react",
  "version": "0.0.0-development",
  "private": false,
  "author": "Immonet dev team <aegaeon@immonet.de> (https://immonet.de)",
  "license": "MIT",
  "description": "JavaScript Standard Style for Immowelt React projects - ESLint Shareable Config",
  "main": "src/index.js",
  "scripts": {
    "test": "npm run lint && npm run jest:coverage",
    "lint": "eslint src",
    "jest": "jest",
    "jest:watch": "npm run jest -- --watch",
    "jest:coverage": "npm run jest -- --coverage",
    "semantic-release": "semantic-release pre && npm publish --access=public && semantic-release post"
  },
  "dependencies": {
    "babel-eslint": "7.2.3",
    "eslint-config-xo": "^0.18.2",
    "eslint-config-xo-react": "^0.13.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0"
  },
  "devDependencies": {
    "@immowelt/jest-preset-node": "^1.0.0",
    "@inkdpixels/commit-analyzer": "^1.0.0",
    "@inkdpixels/release-notes-generator": "^1.0.0",
    "babel-jest": "20.0.3",
    "eslint": "4.4.1",
    "jest": "^20.0.4",
    "semantic-release": "^7.0.1"
  },
  "peerDependencies": {
    "eslint": "4.4.1"
  },
  "engines": {
    "node": ">=8.2.0",
    "npm": ">=5.3.0"
  },
  "release": {
    "analyzeCommits": "@inkdpixels/commit-analyzer",
    "generateNotes": "@inkdpixels/release-notes-generator"
  },
  "browserslist": [
    "last 1 versions"
  ],
  "jest": {
    "preset": "@immowelt/jest-preset-node"
  },
  "keywords": [
    "eslint",
    "immowelt",
    "config",
    "immonet"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ImmoweltGroup/eslint-config-immowelt-react.git"
  }
}
