{
  "name": "@immowelt/eslint-config-immowelt-react",
  "version": "0.0.0",
  "description": "ESLint shareable react config",
  "main": "index.js",
  "repository": "https://github.com/ImmoweltGroup/eslint-config-immowelt-react.git",
  "author": "Kevin Ruff <kevin.ruff@immonet.de>",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=12.0.0",
    "yarn": ">=1.22.0"
  },
  "dependencies": {
    "@immowelt/eslint-config-immowelt-base": "1.4.3",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1"
  },
  "devDependencies": {
    "semantic-release": "^15.9.17"
  },
  "scripts": {
    "lint": "eslint .",
    "release": "semantic-release"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master",
    "analyzeCommits": {
      "releaseRules": [
        {
          "type": "BREAKING",
          "release": "major"
        },
        {
          "type": "FEATURE",
          "release": "minor"
        },
        {
          "type": "BUGFIX",
          "release": "patch"
        },
        {
          "type": "SECURITY",
          "release": "patch"
        }
      ]
    }
  },
  "keywords": [
    "eslint",
    "config",
    "immowelt",
    "react"
  ]
}
