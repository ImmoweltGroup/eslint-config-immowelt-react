{
  "name": "@immowelt/eslint-config-immowelt-react",
  "version": "0.0.0-development",
  "private": false,
  "author": "Immonet dev team <aegaeon@immonet.de> (https://immonet.de)",
  "license": "MIT",
  "description": "JavaScript Standard Style for Immowelt React projects - ESLint Shareable Config",
  "main": "src/index.js",
  "scripts": {
    "test": "npm run lint && npm run jest:coverage",
    "lint": "eslint src",
    "prettier": "prettier --write src/**/*.js",
    "jest": "jest",
    "jest:watch": "npm run jest -- --watch",
    "jest:coverage": "npm run jest -- --coverage",
    "semantic-release": "semantic-release pre && npm publish --access=public && semantic-release post",
    "precommit": "lint-staged && npm test"
  },
  "lint-staged": {
    "src/**/*.{js,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "babel-eslint": "8.0.2",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-compat": "^2.0.1",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-standard": "^3.0.1"
  },
  "devDependencies": {
    "@immowelt/jest-preset-node": "^1.0.0",
    "@immowelt/styleguide-javascript": "^1.0.5",
    "@inkdpixels/commit-analyzer": "^1.0.0",
    "@inkdpixels/release-notes-generator": "^1.0.0",
    "eslint": "4.10.0",
    "babel-jest": "21.2.0",
    "husky": "^0.14.3",
    "jest": "^21.0.2",
    "lint-staged": "^5.0.0",
    "prettier": "^1.6.1",
    "semantic-release": "^8.0.0"
  },
  "peerDependencies": {
    "eslint": "4.4.1"
  },
  "engines": {
    "node": ">=8.2.0",
    "npm": ">=5.3.0"
  },
  "release": {
    "analyzeCommits": "@inkdpixels/commit-analyzer",
    "generateNotes": "@inkdpixels/release-notes-generator"
  },
  "browserslist": [
    "last 1 versions"
  ],
  "jest": {
    "preset": "@immowelt/jest-preset-node"
  },
  "keywords": [
    "eslint",
    "immowelt",
    "config",
    "immonet"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ImmoweltGroup/eslint-config-immowelt-react.git"
  }
}
